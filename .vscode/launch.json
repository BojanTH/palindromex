    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env":
                {
                    "JWT_KEY": "someKey",
                    "SESSION_SECRET": "someSecret",
                    "DB_CONNECTION": "host=localhost user=root password=root dbname=postgres port=5432 sslmode=disable",
                    "PORT": "8080"
                },
            "args": [
                "serve"
           ]
        }
    ]
}